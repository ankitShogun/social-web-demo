  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "dev": "DISABLE_ESLINT_PLUGIN=true WATCHPACK_POLLING=true PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
